@import "my-sass-resources/base";


/**
 * Custom Sassified Theme for reveal.js.
 *
 * Copyright (C) 2011-2012 Hakim El Hattab, http://hakim.se
 */

/*********************************************
 * THEME VARIABLES
 *********************************************/

$body-radial-background-color-inner: #b2e4ad;
$body-radial-background-color-outer: #fafcf7;

// Bluish Green
$body-radial-background-color-inner-1: #ade4cf;
$body-radial-background-color-outer-1: #f7fcfb;

// Orange
$body-radial-background-color-inner-2: #e4d5ad;
$body-radial-background-color-outer-2: #fcfbf7;

// Green
$body-radial-background-color-inner-3: #add9e4;
$body-radial-background-color-outer-3: #f7fbfc;

// Pink
$body-radial-background-color-inner-4: #e4adc3;
$body-radial-background-color-outer-4: #fcf7f9;

$body-font: "Open Sans", sans-serif;
$title-font: 'Oxygen', sans-serif;


.reveal a:not(.image) {
  line-height: 1.3em; }

/*********************************************
 * GLOBAL STYLES
 *********************************************/
body {
  background:  $body-radial-background-color-inner;
  background: -moz-radial-gradient(center, circle cover, $body-radial-background-color-outer 0%,  $body-radial-background-color-inner 100%);
  background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%, $body-radial-background-color-outer), color-stop(100%,  $body-radial-background-color-inner));
  background: -webkit-radial-gradient(center, circle cover, $body-radial-background-color-outer 0%,  $body-radial-background-color-inner 100%);
  background: -o-radial-gradient(center, circle cover, $body-radial-background-color-outer 0%,  $body-radial-background-color-inner 100%);
  background: -ms-radial-gradient(center, circle cover, $body-radial-background-color-outer 0%,  $body-radial-background-color-inner 100%);
  background: radial-gradient(center, circle cover, $body-radial-background-color-outer 0%,  $body-radial-background-color-inner 100%);
  background-color: $body-radial-background-color-outer; }

.bgrd-alternate-1 body {
	background:  $body-radial-background-color-inner-1;
  background: -moz-radial-gradient(center, circle cover, $body-radial-background-color-outer-1 0%,  $body-radial-background-color-inner-1 100%);
  background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%, $body-radial-background-color-outer-1), color-stop(100%,  $body-radial-background-color-inner-1));
  background: -webkit-radial-gradient(center, circle cover, $body-radial-background-color-outer-1 0%,  $body-radial-background-color-inner-1 100%);
  background: -o-radial-gradient(center, circle cover, $body-radial-background-color-outer-1 0%,  $body-radial-background-color-inner-1 100%);
  background: -ms-radial-gradient(center, circle cover, $body-radial-background-color-outer-1 0%,  $body-radial-background-color-inner-1 100%);
  background: radial-gradient(center, circle cover, $body-radial-background-color-outer-1 0%,  $body-radial-background-color-inner-1 100%);
  background-color: $body-radial-background-color-outer-1; 
}

.bgrd-alternate-2 body {
	background:  $body-radial-background-color-inner-2;
  background: -moz-radial-gradient(center, circle cover, $body-radial-background-color-outer-2 0%,  $body-radial-background-color-inner-2 100%);
  background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%, $body-radial-background-color-outer-2), color-stop(100%,  $body-radial-background-color-inner-2));
  background: -webkit-radial-gradient(center, circle cover, $body-radial-background-color-outer-2 0%,  $body-radial-background-color-inner-2 100%);
  background: -o-radial-gradient(center, circle cover, $body-radial-background-color-outer-2 0%,  $body-radial-background-color-inner-2 100%);
  background: -ms-radial-gradient(center, circle cover, $body-radial-background-color-outer-2 0%,  $body-radial-background-color-inner-2 100%);
  background: radial-gradient(center, circle cover, $body-radial-background-color-outer-2 0%,  $body-radial-background-color-inner-2 100%);
  background-color: $body-radial-background-color-outer-2; 
}

.bgrd-alternate-3 body {
	background:  $body-radial-background-color-inner-3;
  background: -moz-radial-gradient(center, circle cover, $body-radial-background-color-outer-3 0%,  $body-radial-background-color-inner-3 100%);
  background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%, $body-radial-background-color-outer-3), color-stop(100%,  $body-radial-background-color-inner-3));
  background: -webkit-radial-gradient(center, circle cover, $body-radial-background-color-outer-3 0%,  $body-radial-background-color-inner-3 100%);
  background: -o-radial-gradient(center, circle cover, $body-radial-background-color-outer-3 0%,  $body-radial-background-color-inner-3 100%);
  background: -ms-radial-gradient(center, circle cover, $body-radial-background-color-outer-3 0%,  $body-radial-background-color-inner-3 100%);
  background: radial-gradient(center, circle cover, $body-radial-background-color-outer-3 0%,  $body-radial-background-color-inner-3 100%);
  background-color: $body-radial-background-color-outer-3; 
}

.bgrd-alternate-4 body {
	background:  $body-radial-background-color-inner-4;
  background: -moz-radial-gradient(center, circle cover, $body-radial-background-color-outer-4 0%,  $body-radial-background-color-inner-4 100%);
  background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%, $body-radial-background-color-outer-4), color-stop(100%,  $body-radial-background-color-inner-4));
  background: -webkit-radial-gradient(center, circle cover, $body-radial-background-color-outer-4 0%,  $body-radial-background-color-inner-4 100%);
  background: -o-radial-gradient(center, circle cover, $body-radial-background-color-outer-4 0%,  $body-radial-background-color-inner-4 100%);
  background: -ms-radial-gradient(center, circle cover, $body-radial-background-color-outer-4 0%,  $body-radial-background-color-inner-4 100%);
  background: radial-gradient(center, circle cover, $body-radial-background-color-outer-4 0%,  $body-radial-background-color-inner-4 100%);
  background-color: $body-radial-background-color-outer-4; 
}


.reveal {
  font-family: $body-font;
  font-size: 36px;
  font-weight: 200;
  letter-spacing: -0.02em;
  color: #333333; }

::selection {
  color: white;
  background: #134674;
  text-shadow: none; }

.reveal .resources {
	font-size: .75em;
	width: 50%;
	margin-top: 3em;
	ul {
		@include clean-list;
	}
}


/*********************************************
 * HEADERS
 *********************************************/
.reveal h1,
.reveal h2,
.reveal h3,
.reveal h4,
.reveal h5,
.reveal h6 {
  margin: 0 0 20px 0;
  color: #333333;
  font-family: $title-font;
  line-height: 0.9em;
  letter-spacing: -0.08em;
  text-shadow: none; }

.reveal h1 {
  text-shadow: 0px 0px 6px rgba(0, 0, 0, 0.2);
	font-size: 3.77em;
}

.reveal h2{
	font-size:2.11em;
}

.reveal h3{
	font-size:1.55em;
}

.reveal h4{
	font-size:1em;
}

.reveal p {
	line-height: 1.5;
}

.reveal .topic-title {
	position: absolute;
	top: -36px;
	left: -20px;
	font-size: .5em;
	line-height: 1;
	background: $body-radial-background-color-inner;
	border: 1px solid darken($body-radial-background-color-inner, 10%);
	@include box-shadow(0 2px 1px rgba(255,255,255, .2));
	padding: .35em .25em .1em .25em;
}

.large-link-wrpr {
	@include clean-list;
	a {
		display: inline-block;
		font-size: .75em;
		padding: 0 1em;
		background-color: rgba(0,0,0,.05);
		border: 1px solid rgba(0,0,0,.075);
		@include box-shadow(0 2px 1px rgba(255,255,255, .2));
		text-transform: lowercase;
		&:hover {
			color: $body-radial-background-color-outer-1;
		}
	}
} 

.link-button {
		display: inline-block;
		font-size: .75em;
		padding: 0 1em;
		background-color: rgba(0,0,0,.05);
		border: 1px solid rgba(0,0,0,.075);
		@include box-shadow(0 2px 1px rgba(255,255,255, .2));
		text-transform: lowercase;
		&:hover {
			color: $body-radial-background-color-outer-1;
		}
}


.bgrd-alternate-1 .reveal .topic-title {
	background: $body-radial-background-color-inner-1;
	border: 1px solid darken($body-radial-background-color-inner-1, 10%);
}


/*********************************************
 * LINKS
 *********************************************/
.reveal a:not(.image) {
  color: #3b759e;
  text-decoration: none;
  -webkit-transition: color .15s ease;
  -moz-transition: color .15s ease;
  -ms-transition: color .15s ease;
  -o-transition: color .15s ease;
  transition: color .15s ease; }

.reveal a:not(.image):hover {
  color: #74a7cb;
  text-shadow: none;
  border: none; }

.reveal .roll span:after {
  color: #fff;
  background: #264c66; }

/*********************************************
 * IMAGES
 *********************************************/
.reveal section img {
  margin: 15px 0px;
  background: rgba(255, 255, 255, 0.12);
  border: 4px solid #333333;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.15);
  -webkit-transition: all .2s linear;
  -moz-transition: all .2s linear;
  -ms-transition: all .2s linear;
  -o-transition: all .2s linear;
  transition: all .2s linear; }

.reveal a:hover img {
  background: rgba(255, 255, 255, 0.2);
  border-color: #3b759e;
  box-shadow: 0 0 20px rgba(0, 0, 0, 0.55); }

/*********************************************
 * NAVIGATION CONTROLS
 *********************************************/
.reveal .controls div.navigate-left,
.reveal .controls div.navigate-left.enabled {
  border-right-color: #3b759e; }

.reveal .controls div.navigate-right,
.reveal .controls div.navigate-right.enabled {
  border-left-color: #3b759e; }

.reveal .controls div.navigate-up,
.reveal .controls div.navigate-up.enabled {
  border-bottom-color: #3b759e; }

.reveal .controls div.navigate-down,
.reveal .controls div.navigate-down.enabled {
  border-top-color: #3b759e; }

.reveal .controls div.navigate-left.enabled:hover {
  border-right-color: #74a7cb; }

.reveal .controls div.navigate-right.enabled:hover {
  border-left-color: #74a7cb; }

.reveal .controls div.navigate-up.enabled:hover {
  border-bottom-color: #74a7cb; }

.reveal .controls div.navigate-down.enabled:hover {
  border-top-color: #74a7cb; }

/*********************************************
 * PROGRESS BAR
 *********************************************/
.reveal .progress {
  background: rgba(0, 0, 0, 0.2); }

.reveal .progress span {
  background: #3b759e;
  -webkit-transition: width 800ms cubic-bezier(0.26, 0.86, 0.44, 0.985);
  -moz-transition: width 800ms cubic-bezier(0.26, 0.86, 0.44, 0.985);
  -ms-transition: width 800ms cubic-bezier(0.26, 0.86, 0.44, 0.985);
  -o-transition: width 800ms cubic-bezier(0.26, 0.86, 0.44, 0.985);
  transition: width 800ms cubic-bezier(0.26, 0.86, 0.44, 0.985); }

